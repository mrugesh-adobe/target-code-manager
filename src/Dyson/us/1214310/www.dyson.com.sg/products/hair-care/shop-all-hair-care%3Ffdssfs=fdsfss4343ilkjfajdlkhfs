    var addToCart;
    require([
        'jquery',
        'jquery/ui'
    ], function ($) {
        document.addEventListener("readystatechange", (event) => {
            if (event.target.readyState === "complete") {
                $(".toCart").prop('disabled',false);
            }
        });
        addToCart = function (product_id)
        {
            if(!$('.tocart_'+product_id).prop('disabled')){
              if($("#product_addtocart_form_" + product_id).length != 0) {
                $('#product_addtocart_form_'+product_id).submit();
              } else {
                $('.tocart_'+ product_id).click();
              }
            }
