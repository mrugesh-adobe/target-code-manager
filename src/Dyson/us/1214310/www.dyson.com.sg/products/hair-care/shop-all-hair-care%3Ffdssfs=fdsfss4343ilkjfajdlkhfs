require([
  'jquery',
  'mage/mage',
  'mage/validation'
], function ($) {
        $(document).ready(function(){
          var $prospect_form = $('.prospect-form');
          var phone_dial_code_validation = $('.prospect-form').validation({
                errorPlacement: function (error, element) {
                  element.attr("name") == "phone_number" && $(".registration-dial-code").length ? error.insertAfter('.registration-dial-code') : error.insertAfter(element);
                }
            });
            $prospect_form.submit(function() {
                phone_dial_code_validation.validate();
                phone_dial_code_validation.validation("isValid");
            });
            $('.prospect-form').on('submit',function(){
                if(typeof ga !== "undefined"){
                  var clientId = ga.getAll()[0].get('clientId');
                  if(clientId) {
                    $('#web_analytics_id').val(clientId);
                  }
                }
                setTimeout(postcodeValidate,0);
            });
            jQuery('.prospect-form input').on('blur', function() {
                jQuery(this).valid();
                if (!jQuery(this).hasClass('valid')) {
                    var errorid = jQuery(this).attr("id");
                    jQuery(this).siblings('#'+errorid+'-error').attr('role', 'alert');
                    postcodeValidate();
                }
                if (jQuery(this).hasClass('valid')) {
                    jQuery(this).attr("aria-invalid","false");
                }
            });
            /* custom validation check for empty postocde field */
            function postcodeValidate(){
                const postcodeVal = document.querySelector("#postcode").value;
                if(postcodeVal != undefined && postcodeVal != null && postcodeVal.length > 0){
                    document.querySelector("#postcode-error").innerHTML = 'Please enter a %s-digit number';
                }
            }
        });
    });</script><script type="text/x-magento-init">
{
    "*": {
        "Magento_Ui/js/core/app": {
