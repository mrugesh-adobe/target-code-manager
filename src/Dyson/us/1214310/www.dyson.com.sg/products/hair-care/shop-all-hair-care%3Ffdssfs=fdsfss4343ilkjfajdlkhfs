    require(['jquery'], function($){
        $(document).ready(function(){
            var vid = document.getElementById("video4964");
                        vid.play();
            $('#video-play-btn4964').addClass('active');
            $('#video-play-btn4964').addClass('paused');
              vid.onended = function() {
                $('#video-play-btn4964').trigger('click');
            };

        });

        $('#video-play-btn4964').on('click',function(){
            var vid = document.getElementById("video4964");
            if($(this).hasClass('active')){
                $('#video4964').show();
                vid.pause();
                $(this).removeClass('active');
                $(this).removeClass('paused');
                $('#play_icon_btn4964').focus();
            }else{
                $('#video4964').show();
                vid.play();
                $(this).addClass('active');
                $(this).addClass('paused');
                var isAutoPlay = $('#video_play_status4964').val();
                if(isAutoPlay != 1){
                    $('#video4964').wrap('<div class="video-container4964"></div>');
                    $('#video_play_status4964').val(1);
                }
                $('#pause_icon_btn4964').focus();
            }
        });

        $('#video_play_status4964').val(1);
